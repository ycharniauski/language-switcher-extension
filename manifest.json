{
  "name": "Language Switcher",
  "manifest_version": 3,
  "version": "1.0",
  "permissions": [
		"cookies",
		"tabs",
		"storage",
    "scripting"
	],
  "offline_enabled": true,
  "host_permissions": ["https://*/*", "http://*/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "icons/leaf.png",
    "48": "icons/leaf.png"
	}
}
